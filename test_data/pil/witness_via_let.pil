let N = 16;

namespace Fibonacci(N);
    col fixed ISLAST(i) { if i == N - 1 { 1 } else { 0 } };

    let x;
    let y;

    ISLAST * (y' - 1) = 0;
    ISLAST * (x' - 1) = 0;

    (1-ISLAST) * (x' - y) = 0;
    (1-ISLAST) * (y' - (x + y)) = 0;

    public out = y(N - 1);

    ISLAST * (out - y) = 0;
