let N: int = 16;

namespace std::convert(16);
    let fe = [];

namespace std::prover(16);
    enum Query {
        Input(int)
    }

namespace Quad(N);
    col fixed id(i) { i };
    col fixed double(i) { i * 2 };

    col witness input(i) query std::prover::Query::Input(i);
    col witness wdouble;
    col witness quadruple;

    [input, wdouble] in [id, double];
    [wdouble, quadruple] in [id, double];

    public out = quadruple(N-1);
