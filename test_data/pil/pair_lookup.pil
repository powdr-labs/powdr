constant %N = 65536;

namespace Main(%N);
	col fixed A(i) { i % 256 };
	col fixed B(i) { (i >> 8) % 256 };
	col fixed C(i) { (A(i) + B(i)) & 0xff };


	col fixed a(i) { (i + 13) * 29 & 0xff };
	col fixed b(i) { (i * 17 + 3) & 0xff };
	col witness c;

	{a, b, c} in {Main.A, Main.B, Main.C};
	{b, a, c} in {Main.A, Main.B, Main.C};

namespace main(%N);
	col fixed first_step = [1] + [0]*;